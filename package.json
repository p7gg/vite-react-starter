{
	"name": "vite-react-typescript-starter",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview",
		"prettier:check": "prettier --check \"**/*.{js,ts,json,css,tsx,jsx,md,html}\" --ignore-unknown --ignore-path .gitignore",
		"prettier:fix": "prettier --write \"**/*.{js,ts,json,css,tsx,jsx,md,html}\" --ignore-unknown --ignore-path .gitignore",
		"lint:check": "eslint --cache --ext ts,tsx --report-unused-disable-directives --max-warnings 0 src",
		"lint:fix": "eslint --cache --fix --ext ts,tsx --report-unused-disable-directives --max-warnings 0 src",
		"check": "pnpm lint:check && pnpm prettier:check",
		"fix": "pnpm lint:fix && pnpm prettier:fix",
		"clean": "git clean -xdf node_modules dist"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@ianvs/prettier-plugin-sort-imports": "^4.1.0",
		"@types/eslint": "^8.44.2",
		"@types/node": "^20.6.3",
		"@types/react": "^18.2.22",
		"@types/react-dom": "^18.2.7",
		"@typescript-eslint/eslint-plugin": "^6.7.2",
		"@typescript-eslint/parser": "^6.7.2",
		"@vitejs/plugin-react": "^4.0.4",
		"@vitejs/plugin-react-swc": "^3.3.2",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.49.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.3",
		"postcss": "^8.4.30",
		"prettier": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"tailwindcss": "^3.3.3",
		"typescript": "^5.2.2",
		"vite": "^4.4.9"
	}
}
